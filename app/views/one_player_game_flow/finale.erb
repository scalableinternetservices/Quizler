<% content_for :header do %>
    <h1> YEAH </h1>
<% end %>

<h3> You got <%= @total_correct_answers_count%> correct answers of <%= @questions_count%> </h3>
<%= button_to "Main Menu", root_path, :class => "button", :method => :get %>

<table class="table text-left">
  <thead>
  <tr>
    <th>Rank</th>
    <th>User</th>
    <th>Score</th>
  </tr>
  </thead>

  <tbody>
  <% @highscore.each do |hs| %>
      <tr>
        <td><%= 1 %></td>
        <td><%= hs.user_id %></td>
        <td><%= hs.score %></td>
      </tr>
  <% end %>
  </tbody>
</table>